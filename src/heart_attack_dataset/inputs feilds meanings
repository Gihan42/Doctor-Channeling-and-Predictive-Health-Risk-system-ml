  "exang": 1,
  "oldpeak": 2.5,
  "slope": 0,
  "ca": 2,
  "thal": 3
},
{
  "age": 65,
  "sex": 1,        // Male
  "cp": 3,         // Typical angina
  "trtbps": 160,   // High blood pressure
  "chol": 300,     // High cholesterol
  "fbs": 1,        // High blood sugar
  "restecg": 1,    // Abnormal ECG
  "thalach": 120,  // Low max heart rate
  "exang": 1,      // Exercise-induced angina
  "oldpeak": 2.5,  // Significant ST depression
  "slope": 0,      // Downsloping
  "ca": 2,         // Two major vessels blocked
  "thal": 3        // Abnormal thalassemia test
}